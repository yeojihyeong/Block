<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.micol.syperest.service.MemberMapper">

	<select id="memberSelectList" resultType="co.micol.syperest.service.MemberVO">
	 SELECT * FROM MEMBER
	</select>
	
	<select id="memberSelect" parameterType="co.micol.syperest.service.MemberVO" resultType="co.micol.syperest.service.MemberVO">
		SELECT * FROM MEMBER WHERE MEMBER_ID=#{member_id}
		<if test="password != null">
			AND member_PASSWORD = #{member_passowrd}
		</if>
	</select>
	
	<insert id="memberInsert"></insert>
	
	<update id="memberUpdate"></update>
	
	<delete id="memberDelete"></delete>

</mapper>